<!doctype html>
<html>
<head>
    <title>学生页面</title>
    <style>
        * { margin: 0; padding: 0; box-sizing: border-box; }
        body { font: 13px Helvetica, Arial; }
        form { background: #000; padding: 3px; position: fixed; bottom: 0; width: 100%; }
        form input { border: 0; padding: 10px; width: 90%; margin-right: .5%; }
        form button { width: 9%; background: rgb(130, 224, 255); border: none; padding: 10px; }
        #messages { list-style-type: none; margin: 0; padding: 0; }
        #messages li { padding: 5px 10px; }
        #messages li:nth-child(odd) { background: #eee; }
        div button {margin-left: 50px; margin-top: 15px; font-size: 20px;}
        #classes {margin-left: 40px}
        button.list {margin-left: 30px}
        #classInfo {margin-left: 40px ; font-size: 16px}
    </style>

</head>
<body>
    <ul id="classes"></ul>
    <div>
        <button id="late">我迟到了</button>
        <button id="offline">我离线观看</button>
    </div>
    <div>
        <button id="pause">暂停一下</button>
        <button id="restart">再开始</button>
    </div>

    <ul id="messages"></ul>

    <!--form action="" method="post">
        <input id="m" autocomplete="off" /><button id="aa">Send</button>
    </form-->


<script src="https://cdn.socket.io/socket.io-1.3.4.js"></script>
<script src="javascripts/jquery-2.0.3.min.js"></script>
<script src="javascripts/replayControl.js"></script>
<script src="javascripts/stuSocketFront.js"></script>
</body>
</html>